<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideofgujarat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TopSights"
            android:label="Top Sights"/>
        <activity android:name=".FamousRestaurants"
            android:label="Famous Restaurants"/>
        <activity android:name=".HistoricalMonuments"
            android:label="Historical Monuments"/>
        <activity android:name=".UpcomingEvents"
            android:label="Upcoming Events"/>
    </application>

</manifest>